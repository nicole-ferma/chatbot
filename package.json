{
  "name": "chatbot",
  "version": "1.0.0",
  "description": "a chatbot app",
  "main": "index.js",
  "scripts": {
    "start": "run-p webpack server",
    "dev": "run-p client server",
    "build": "knex migrate:latest",
    "webpack": "webpack --config client/webpack.config.js",
    "client": "npm run webpack -- --watch",
    "server": "nodemon server",
    "lint": "eslint --ext .js,.jsx .",
    "knex": "knex --knexfile server/db/knexfile.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --noStackTrace --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicole-ferma/chatbot.git"
  },
  "keywords": [],
  "author": "Nicole Ferma",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nicole-ferma/chatbot/issues"
  },
  "homepage": "https://github.com/nicole-ferma/chatbot#readme",
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.0",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/superagent": "^4.1.15",
    "babel-loader": "^8.2.5",
    "eslint": "^8.17.0",
    "eslint-config-eda": "^1.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.2",
    "nock": "^13.2.7",
    "nodemon": "^2.0.18",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "superagent": "^7.1.6",
    "supertest": "^6.2.4",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "express": "^4.18.1",
    "knex": "^2.1.0",
    "pg": "^8.7.3",
    "sqlite3": "^5.0.8"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true
  },
  "browserslist": "> 2%, not dead",
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "engines": {
    "node": "16.x"
  }
}
